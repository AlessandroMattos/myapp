<p id="notice"><%= notice %></p>

<h1>Moedas Ja</h1>

<table class="table">
  <thead>
    <tr>
      <th>Description</th>
      <th>Acrony</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @moedas.each do |moeda| %>
      <tr>
        <td><%= moeda.description %></td>
        <td><%= moeda.acrony %></td>
        <td><img src="<%= moeda.url_image%>" width=50px heigth=50px/></td>
        <td><%= link_to 'Show', moeda %></td>
        <td><%= link_to 'Edit', edit_moeda_path(moeda) %></td>
        <td><%= link_to 'Destroy', moeda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Moeda', new_moeda_path %>
